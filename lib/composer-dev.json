{
  "name": "51degrees/fiftyonedegrees.wordpress",
  "description": "51Degrees Pipeline API",
  "authors": [
    {
      "name": "Fatima Tariq",
      "email": "fatima@51degrees.com"
    }
  ],
  "license": "EUPL-1.2",
  "repositories": [
    {
      "type": "path",
      "url": "pipeline-php-cloudrequestengine",
      "options": {
        "versions": {
          "51degrees/fiftyone.pipeline.cloudrequestengine": "4"
        }
      }
    }
  ],
  "require": {
    "51degrees/fiftyone.pipeline.cloudrequestengine": "*",
    "google/apiclient": "^2.10"
  },
  "scripts": {
    "pre-autoload-dump": "Google\\Task\\Composer::cleanup"
  },
  "extra": {
    "google/apiclient-services": [
      "Analytics",
      "AnalyticsData",
      "AnalyticsReporting",
      "GoogleAnalyticsAdmin"
    ]
  },
  "require-dev": {
    "brain/monkey": "^2.6",
    "yoast/phpunit-polyfills": "^1.0"
  }
}
